{
  "name": "jazzman/wp-object-cache",
  "version": "1.0",
  "license": "GPL-3.0+",
  "type": "wordpress-plugin",
  "description": "Enable object caching for WordPress",
  "authors": [
    {
      "name": "Vasyl Sokolyk",
      "email": "vsokolyk@gmail.com"
    }
  ],
  "config": {
    "sort-packages": true,
    "classmap-authoritative": true,
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "apcu-autoloader": true
  },
  "require": {
    "php": ">=7.0",
    "composer/installers": "^1",
    "jazzman/singleton-trait": "^1.1",
    "phpfastcache/phpfastcache": "^7.0"
  },
  "suggest": {
    "ext-apc": "*",
    "ext-intl": "*",
    "ext-memcached": "*",
    "ext-memcache": "*",
    "ext-redis": "*",
    "ext-xcache": "*",
    "ext-sqlite": "*",
    "ext-wincache": "*",
    "ext-leveldb": "*",
    "ext-couchbase": "*",
    "predis/predis": "~1.1.0",
    "mongodb/mongodb": "^1.1",
    "phpfastcache/phpssdb": "~1.0.0",
    "phpfastcache/couchdb": "~1.0.0",
    "phpfastcache/riak-client": "~1.4.4"
  },
  "require-dev": {
    "dg/composer-cleaner": "^2.1",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "JazzMan\\WPObjectCache\\": "src"
    },
    "files": [
      "helper.php"
    ]
  }
}
