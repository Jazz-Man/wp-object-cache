{
    "name": "jazzman/wp-object-cache",
    "type": "wordpress-dropin",
    "authors": [
        {
            "name": "Vasyl Sokolyk",
            "email": "vsokolyk@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/Jazz-Man/wp-memcached.git"
        }

    ],
    "require": {
        "jazzman/wp-memcached": "dev-master"
    },
    "extra": {
        "installer-paths": {
            "vendor/wp/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
            "vendor/wp/plugins/{$name}/": ["type:wordpress-plugin"],
            "vendor/wp/themes/{$name}/": ["type:wordpress-theme"]
        },
        "wordpress-install-dir": "vendor/wp"
    },
    "require-dev": {
        "roots/wordpress": "^4"
    }
}
